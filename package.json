{
  "name": "todo-react-playwright",
  "version": "0.3.2",
  "private": true,
  "homepage": "http://localhost:3000/",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
